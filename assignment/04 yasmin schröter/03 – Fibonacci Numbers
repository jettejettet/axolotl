number = int(input("Please enter a number: "))
n1, n2 = 0, 1
count = 0

if number < 0:
    print("Please enter a positive integer")
elif number == 0:
  print(0)
elif number == 1:
    print("Fibonacci sequence up to",number,":")
    print(n1)
else:
    print("Fibonacci sequence:")
    while count < number:
        print(n1)
        nth = n1 + n2
        n1 = n2
        n2 = nth
        count += 1
